    "main": "index.js",
    "scripts": {
        "format": "npx prettier --write \"**/*.{ts,js,json,html,css,cjs,mjs}\" --config ./.prettierrc",
        "prebuild": "ts-node ./src/prebuild-scripts/updateAppVersion.script.ts",
        "build": "dotenv -e .env.production npx tsc",
        "build:dev": "dotenv -e .env.development npx tsc",
        "build:stage": "dotenv -e .env.staging npx tsc",
        "start": "npm run build && dotenv -e .env.production node dist/src/index.js",
        "start:dev": "npm run build:dev && dotenv -e .env.development node dist/src/index.js",
        "start:stage": "npm run build:stage && dotenv -e .env.staging node dist/src/index.js",
        "dev": "dotenv -e .env.development nodemon src/index.ts",
        "dev:stage": "dotenv -e .env.staging nodemon src/index.ts",
        "dev:prod": "./confirm-prod.sh"
    },
